          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>宣言</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>